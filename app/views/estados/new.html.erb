<h1>New estado</h1>

<div class="row">
  <div id="formrow">
    <%= form_for(@estado, :html => { :multipart => true }) do |f| %>

        <%= render 'shared/error_messages', object: f.object %>


          <%= f.label :estado %>
          <%= f.text_field :estado %>

          <%= f.submit "Sube tu evento", class: "btn btn-large btn-primary" %>

    <% end %>

<%= link_to 'Back', estados_path %>